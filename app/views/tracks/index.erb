<h1>All Tracks</h1>
<hr/>

<% @tracks.each do |track| %>
  <div class="track">
    <h3><a href="/tracks/<%= tracks.id %>"><%= track.title %></a> <small>(by: <%= track.author %>)</small></h3>
    <p>
      <%= track.url %>
    </p>
  </div>
<% end %>

<hr/>
<a href="/tracks/new">Post a new track.</a>